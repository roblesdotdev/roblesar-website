---
import BaseLayout from './BaseLayout.astro'

type Props = {
  frontmatter: {
    title: string
    description: string
  }
}
const { frontmatter } = Astro.props
---

<BaseLayout title={frontmatter.title} description={frontmatter.description}>
  <div class="prose-mdx prose !max-w-none">
    <slot />
  </div>
</BaseLayout>
