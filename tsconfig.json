{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "types": ["@cloudflare/workers-types/2023-07-01"],
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/**", ".vscode", "dist"]
}
